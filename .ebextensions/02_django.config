    DJANGO_SETTINGS_MODULE: "app.settings.prod"
    PYTHONPATH: "/opt/python/current/app/contractmanager:$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: "wsgi.py"

container_commands:
  01_collectstatic:
    command: "python contractmanager/manage.py collectstatic --noinput"
